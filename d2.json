        "prefix": "style",
        "body": [
            "\tstyle: {",
            "\t\t# opacity: 0.4,\t\t#",
            "\t\t# stroke: \"#f4a261\",   #",
            "\t\t# fill: \"#f4a261\",\t # (shape only)",
            "\t\t# stroke-width: 1,\t #",
            "\t\t# stroke-dash: 5,\t  #",
            "\t\t# border-radius: 3,\t# (shape only)",
            "\t\t# shadow: false,\t   # (shape only)",
            "\t\t# 3D: false,\t\t   # (rectangle/square only)",
            "\t\t# multiple: false,\t # (shape only)",
            "\t\t# font-size: 28,\t   #",
            "\t\t# font-color: red,\t #",
            "\t\t# animated: false,\t # (connection only)",
            "\t\t# bold: false,\t\t #",
            "\t\t# italic: false,\t   #",
            "\t\t# underline: false\t #",
            "\t}",
            ""
        ],
        "description": "Inserts a Style with All Default Values"
    },
